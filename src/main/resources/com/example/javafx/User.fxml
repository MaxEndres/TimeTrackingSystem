<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx.UserProfile">
   <children>
      <Label layoutX="288.0" layoutY="38.0" text="Time" />
      <Label fx:id="timeLabel" layoutX="249.0" layoutY="74.0" text="Timer">
         <font>
            <Font size="31.0" />
         </font>
      </Label>
      <Button fx:id="startButton" layoutX="221.0" layoutY="132.0" mnemonicParsing="false" onAction="#startButtonOnAction" prefHeight="25.0" prefWidth="62.0" text="Start" />
      <Button fx:id="pauseButton" layoutX="317.0" layoutY="132.0" mnemonicParsing="false" onAction="#pauseButtonOnAction" prefHeight="25.0" prefWidth="62.0" text="Pause" />
      <Button fx:id="stopButton" layoutX="249.0" layoutY="168.0" mnemonicParsing="false" onAction="#stopButtonOnAction" prefHeight="25.0" prefWidth="103.0" text="Stop" />
      <Button layoutX="62.0" layoutY="292.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="103.0" text="Edit Time" />
      <Button layoutX="249.0" layoutY="292.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="103.0" text="View Time" />
      <Button layoutX="434.0" layoutY="292.0" mnemonicParsing="false" text="Contact Support" />
   </children>
</AnchorPane>
