<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffff;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx.EditRequest">
   <children>
      <TableView fx:id="timestampsTableView" layoutX="36.0" layoutY="82.0" prefHeight="200.0" prefWidth="527.0">
         <columns>
            <TableColumn fx:id="timestampIdTableColumn" prefWidth="117.33334350585938" text="TimeStampID" />
            <TableColumn fx:id="dateTableColumn" prefWidth="110.0" text="Date" />
            <TableColumn fx:id="startTableColumn" prefWidth="128.66668701171875" text="Start" />
            <TableColumn fx:id="stopTableColumn" prefWidth="171.33331298828125" text="Time" />
         </columns>
      </TableView>
      <Button fx:id="changeButton" layoutX="78.0" layoutY="319.0" mnemonicParsing="false" onAction="#changeButtonOnAction" prefHeight="25.0" prefWidth="112.0" text="Change/Delete" />
      <Button fx:id="addNewButton" layoutX="484.0" layoutY="47.0" mnemonicParsing="false" onAction="#addNewButtonOnAction" text="Add new " />
      <Button fx:id="goBackToProfileButton" layoutX="390.0" layoutY="319.0" mnemonicParsing="false" onAction="#goBackToProfileButtonOnAction" prefHeight="25.0" prefWidth="112.0" text="back" />
      <Label fx:id="errorLabel" layoutX="213.0" layoutY="283.0" text="Please select a Timestamp" textFill="#f20303" />
      <Label layoutX="240.0" layoutY="24.0" text="Timestamps" textFill="#3481c0">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Label>
      <ImageView fitHeight="70.0" fitWidth="67.0" layoutX="36.0" layoutY="12.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../assets/img.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
