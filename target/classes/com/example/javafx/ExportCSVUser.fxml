<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="315.0" prefWidth="441.0" style="-fx-background-color: #fcfeff;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx.ExportCSVUser">
   <children>
      <ComboBox fx:id="monthComboBox" layoutX="228.0" layoutY="101.0" prefWidth="150.0" />
      <ComboBox fx:id="yearComboBox" layoutX="228.0" layoutY="137.0" prefWidth="150.0" />
      <Button fx:id="exportButton" layoutX="251.0" layoutY="199.0" mnemonicParsing="false" onAction="#exportButtonOnAction" prefHeight="25.0" prefWidth="103.0" text="Export CSV" />
      <Button fx:id="goBackButton" layoutX="251.0" layoutY="237.0" mnemonicParsing="false" onAction="#goBackButtonOnAction" prefHeight="25.0" prefWidth="103.0" text="Go Back" />
      <Label layoutX="51.0" layoutY="40.0" text="Please select the filtering options to export your worked time" textFill="#3481c0">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font>
      </Label>
      <ImageView fitHeight="132.0" fitWidth="118.0" layoutX="40.0" layoutY="101.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../assets/img.png" />
         </image>
      </ImageView>
      <Label layoutX="174.0" layoutY="106.0" text="Month:" textFill="#3481c0">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="181.0" layoutY="142.0" text="Year:" textFill="#3481c0">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
